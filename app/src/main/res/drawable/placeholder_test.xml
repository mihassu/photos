<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:width="320dp"
                android:height="384dp"/>
            <stroke
                android:width="5dp"
                android:color="@color/colorPrimaryDark">
            </stroke>
        </shape>
    </item>

    <item android:gravity="center"
        android:start="170dp">
        <shape android:shape="oval">
            <size
                android:width="60dp"
                android:height="60dp"/>
            <solid
                android:color="@color/colorPrimaryDark">
            </solid>
        </shape>
    </item>

    <item android:gravity="center">
        <shape android:shape="oval">
            <size
                android:width="40dp"
                android:height="40dp"/>
            <solid
                android:color="@color/colorPrimaryDark">
            </solid>
        </shape>
    </item>

    <item android:gravity="center"
        android:start="-150dp">
        <shape android:shape="oval">
            <size
                android:width="20dp"
                android:height="20dp"/>
            <solid
                android:color="@color/colorPrimaryDark">
            </solid>
        </shape>
    </item>

</layer-list>